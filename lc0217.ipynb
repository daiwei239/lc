    "def readBinaryWatch(self, turnedOn: int) -> List[str]:\n",
    "    res = list()\n",
    "    for i in range(0,12):\n",
    "        for j in range(0,60):\n",
    "            if bin(i).count(\"1\")+bin(j).count(\"1\")==turnedOn:\n",
    "                res.append(f\"{i}:{j:02d}\")\n",
    "    return res\n"
   ]
  }
